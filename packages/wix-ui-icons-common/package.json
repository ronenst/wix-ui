{
  "name": "wix-ui-icons-common",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Ronen Stern",
    "email": "ronens@wix.com"
  },
  "main": "./dist/src/index.js",
  "files": [
    "dist",
    "src",
    "testkit"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "precommit": "haste lint",
    "buildIcons": "svg2react-icon 'src/Icons/raw' 'src/Icons/dist' true",
    "pretest": "npm run buildIcons && haste build",
    "build": "haste lint && npm run buildIcons && haste build && npm run create-aliases",
    "test": "haste test",
    "posttest": "haste lint",
    "start": "npm run buildIcons && haste start",
    "release": "haste release",
    "create-aliases": "node ./scripts/create-aliases.js ./dist/src/Icons/dist/components"
  },
  "devDependencies": {
    "@types/chai": "~4.0.0",
    "@types/mocha": "~2.2.0",
    "@types/prop-types": "~15.5.0",
    "@types/react": "~15.6.0",
    "@types/react-dom": "~15.5.0",
    "chai": "~4.1.0",
    "enzyme": "~2.9.0",
    "gh-pages-auto-release": "~1.1.0",
    "haste-preset-yoshi": "latest",
    "husky": "~0.14.0",
    "import-path": "0.0.13",
    "jsdom": "~11.1.0",
    "prop-types": "~15.6.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "~15.6.0",
    "svg2react-icon": "^1.0.51",
    "typescript": "~2.4.0"
  },
  "haste": {
    "preset": "yoshi"
  },
  "yoshi": {
    "entry": {
      "app": "index.ts"
    },
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM"
    }
  }
}
